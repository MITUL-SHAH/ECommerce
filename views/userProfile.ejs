<!DOCTYPE html>
<html>
	<head>
		<meta charset="ISO-8859-1" />
		<title>Register to ScarletElectronics</title>
		<link
			href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/css/bootstrap.min.css"
			rel="stylesheet" />
		<link
			rel="stylesheet"
			type="text/css"
			href="https://cdn.datatables.net/1.10.25/css/dataTables.bootstrap5.css" />
		<script
			src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"
			type="text/javascript"></script>
	</head>

	<body>
		<div>
			<!-- <br> <br> <br> -->
			<div>
				<ul class="nav">
					<li class="nav-item">
						<form action="/item-listing" method="get">
							<button type="submit" class="btn btn-primary">
								Home
							</button>
						</form>
					</li>
					&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
					<li class="nav-item">
						<form action="/users/modify-user" method="get">
							<button type="submit" class="btn btn-primary">
								Edit
							</button>
						</form>
					</li>
				</ul>
			</div>

			<br />
			<br />
			<div>
				<label for="name" class="form-label text-left"
					><%=user.name%></label
				>
			</div>
			<br />
			<div>
				<label for="phone_nr" class="form-label"><%=user.phone%></label>
			</div>
			<br />
			<div>
				<label for="address_line1" class="form-label text-left"
					><%=user.address1%></label
				>
			</div>
			<br />
			<div>
				<label for="address_line2" class="form-label text-left"
					><%=user.address2%></label
				>
			</div>
			<br />
			<div>
				<label for="address_city" class="form-label text-left"
					><%=user.city%></label
				>
			</div>
			<br />
			<div>
				<div>
					<div>
						<!-- <label for="address_state_code" class="form-label text-left">State</label> <input type="text"
                        class="form-control text-left" id="address_state_code" name="address_state_code" required> -->
						<div>
							<label for="address_state_code"
								><%=user.state%></label
							>
						</div>
					</div>
					<br />
					<div>
						<label for="address_zipcode"><%=user.zipcode%></label>
					</div>
				</div>
			</div>
			<p id="err_para" style="color: red"></p>
			<br />
		</div>
	</body>
</html>
